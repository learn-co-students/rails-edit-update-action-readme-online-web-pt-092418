<h1>My Edit</h1>
<div>
    <%= form_for @article do |f| %>
    <%= f.label 'Article Title' %><br>
    <%= f.text_field :title, id:"title" %><br>

    <%= f.label 'Article Description' %><br>
    <%= f.text_area :description, id:"description" %><br>

    <%= f.submit "Submit Article" %>
  <% end %>
</div>
